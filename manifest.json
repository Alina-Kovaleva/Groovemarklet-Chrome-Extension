{
    "name": "Groovemarklet",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Groovemarklet for Chrome",
    "homepage_url": "http://kampfq.eu",
    "background": {
        "scripts": [
      "background.js"
    ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "Groovemarklet"
    },
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "storage"
  ]
}